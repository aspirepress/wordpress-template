{
    auto_https off
    admin off
    frankenphp
}

:80 {
    root * /app/web

    php_server {
        resolve_root_symlink
    }

    try_files {path} {path}/ /index.php?{query}
    file_server
    encode zstd br gzip

    log {
        output stderr
        format json
    }

    @static {
        path *.css *.js *.ico *.gif *.jpg *.jpeg *.png *.svg *.woff *.woff2
    }
    header @static Cache-Control "public, max-age=31536000"
}
